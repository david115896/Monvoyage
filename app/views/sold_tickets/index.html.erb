<p id="notice"><%= notice %></p>

<h1>Sold Tickets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sold_tickets.each do |sold_ticket| %>
      <tr>
        <td><%= link_to 'Show', sold_ticket %></td>
        <td><%= link_to 'Edit', edit_sold_ticket_path(sold_ticket) %></td>
        <td><%= link_to 'Destroy', sold_ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sold Ticket', new_sold_ticket_path %>
